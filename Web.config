<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="httpplatformhandler" path="*" verb="*" modules="httpPlatformHandler" resourceType="Unspecified" requireAccess="Script" />
        </handlers>
        <httpPlatform stdoutLogEnabled="true" stdoutLogFile="rails.log" startupTimeLimit="20"
                  processesPerApplication="1"
                  processPath="c:\ruby_on_rails\Ruby231\bin\ruby.exe"
                  arguments="&quot;C:\ruby_on_rails\Ruby231\bin\rails&quot; server puma -p %HTTP_PLATFORM_PORT% -b 127.0.0.1">
            <environmentVariables>
                <environmentVariable name="RAILS_ENV" value="development"/>
                <environmentVariable name="RAILS_RELATIVE_URL_ROOT" value="/dimachem"/>
                <environmentVariable name="RAILS_SERVE_STATIC_FILES" value="true"/>
            </environmentVariables>
        </httpPlatform>
    </system.webServer>
</configuration>
